{
    "compilerOptions": {
      "target": "esnext",
      "useDefineForClassFields": true,
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "declaration": true,
      "skipLibCheck": false,
      "composite": true,
      "incremental": true
    },
    "exclude": ["**/tests/**", "**/dist/**", "**/.nuxt/**"],
    "references": [
      {
        "path": "./libs/vite",
        "name": "{{ .Namespace }}"
      }
    ]
  }