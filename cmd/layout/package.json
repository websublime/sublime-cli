  "private": true,
  "scripts": {
    "build": "yarn ui:vite:build && turbo run build",
    "ui:header:start": "turbo run start --filter=@cookyer/ws-ui-header",
    "ui:vite:build": "turbo run build --filter=@cookyer/ws-libs-vite",
    "ui:header:build": "turbo run build --filter=@cookyer/ws-ui-header",
    "ui:common:build": "turbo run build --filter=@cookyer/ws-ui-common",
    "ui:inspector:build": "turbo run build --filter=@cookyer/ws-ui-inspector",
    "release": "yarn changeset publish"
  },
  "devDependencies": {
    "turbo": "^1.2.5",
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.20.0",
    "@changesets/get-github-info": "^0.5.0"
