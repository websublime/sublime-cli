    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Action",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--root", 
                "/Users/miguelramos/Public/Projects/websublime-workspace/sublime-ui", 
                "action", 
                "--kind", 
                "branch", 
                "--bucket", 
                "assets", 
                "--url",
                "websublime/flare",
                "--key", 
                "",
                "--client",
                "github"
            ]
        },
        {
            "name": "Launch Workspace",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "workspace",
                "--name", 
                "ui-libs", 
                "--organization", 
                "websublime", 
                "--repo",
                "websublime/ws-ui-libs", 
            ]
        },
        {
            "name": "Launch Create",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--root", 
                "./ui-libs",
                "create",
                "--name", 
                "ui-bomb", 
                "--type", 
                "pkg", 
                "--template", 
                "lit" 
            ]
        },
        {
            "name": "Launch Register",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "SUBLIME_API_URL": "https://debvasmsyxrewpmqckdv.supabase.co",
                "SUBLIME_API_KEY": ""
            },
            "args": [
                "register",
                "--name", 
                "Miguel Ramos", 
                "--username", 
                "miguelramos", 
                "--email", 
                "miguel@websublime.dev",
                "--password",
                "my_secret_password"
            ]
        },
        {
            "name": "Launch Login",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "SUBLIME_API_URL": "https://debvasmsyxrewpmqckdv.supabase.co",
                "SUBLIME_API_KEY": ""
            },
            "args": [
                "login",
                "--email", 
                "miguel@websublime.dev",
                "--password",
                "my_secret_password"
            ]
        },
        {
            "name": "Launch Root",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": []
        }
    ]
}